          "_name": "features4",
          "_sourceTheme": "wellnessm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features4\" group=\"Features\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    \n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" name=\"showHeading\" title=\"Title\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Button\" name=\"showBtn\" checked>\n        <select title=\"Amount\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/wellnessm5/components/_images/background1.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#232323\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#171410\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper\">\n            <div class=\"col-12\">\n                <h3 class=\"mbr-section-title heading mbr-fonts-style align-center mb-5\" data-app-selector=\".mbr-section-title\" data-app-placeholder=\"Type Text\" mbr-theme-style=\"display-2\" mbr-if=\"showHeading\"><b>SiFU WONG </b>COURSE</h3>\n            </div>\n            <div class=\"row justify-content-center\">\n                <div class=\"col-12 col-lg item features-image active\">\n                    <div class=\"item-wrapper align-center\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/dylan-calluy-mol-kdg0y3w-unsplash.jpg\" alt=\"pic_cryptol\" data-slide-to=\"0\" data-bs-slide-to=\"0\">\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h3 class=\"mbr-title mbr-fonts-style mb-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title, .mbr-section-btn\" mbr-theme-style=\"display-2\">THE CRYPTOL LAB</h3>\n                            <div class=\"mbr-section-btn\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showBtn\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-white-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Learn more<span class=\"effectHover\"></span></a>\n                            </div>\n                        </div>\n                    </div>\n                </div><div class=\"col-12 col-lg item features-image\" mbr-if=\"cardsAmount>1\">\n                    <div class=\"item-wrapper align-center\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/wing-chun.jpeg\" alt=\"pic_wing_chun\" data-slide-to=\"1\" data-bs-slide-to=\"1\">\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h3 class=\"mbr-title mbr-fonts-style mb-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title, .mbr-section-btn\" mbr-theme-style=\"display-2\">KUNG FUNG TRAINER</h3>\n                            <div class=\"mbr-section-btn\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showBtn\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-white-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Learn more<span class=\"effectHover\"></span></a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                \n                <div class=\"col-12 col-lg item features-image\" mbr-if=\"cardsAmount>2\">\n                    <div class=\"item-wrapper align-center\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/the-climate-reality-project-hb6uwq0i4mi-unsplash.jpeg\" alt=\"Pic_SIMPLIFIED\" data-slide-to=\"1\" data-bs-slide-to=\"1\">\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h3 class=\"mbr-title mbr-fonts-style mb-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title, .mbr-section-btn\" mbr-theme-style=\"display-2\">SIMPLIFIED ACADEMY</h3>\n                            <div class=\"mbr-section-btn\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showBtn\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-white-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Learn more<span class=\"effectHover\"></span></a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg item features-image\" mbr-if=\"cardsAmount>3\">\n                    <div class=\"item