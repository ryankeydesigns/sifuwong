or'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper\">\n            <div class=\"col-12\">\n                <h3 class=\"mbr-section-title heading mbr-fonts-style align-center mb-5\" data-app-selector=\".mbr-section-title\" data-app-placeholder=\"Type Text\" mbr-theme-style=\"display-2\" mbr-if=\"showHeading\"><b>SiFU WONG </b>COURSE</h3>\n            </div>\n            <div class=\"row justify-content-center\">\n                <div class=\"col-12 col-lg item features-image active\">\n                    <div class=\"item-wrapper align-center\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/dylan-calluy-mol-kdg0y3w-unsplash.jpg\" alt=\"pic_cryptol\" data-slide-to=\"0\" data-bs-slide-to=\"0\">\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h3 class=\"mbr-title mbr-fonts-style mb-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title, .mbr-section-btn\" mbr-theme-style=\"display-2\">THE CRYPTOL LAB</h3>\n                            <div class=\"mbr-section-btn\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showBtn\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-white-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Learn more<span class=\"effectHover\"></span></a>\n                            </div>\n                        </div>\n                    </div>\n                </div><div class=\"col-12 col-lg item features-image\" mbr-if=\"cardsAmount>1\">\n                    <div class=\"item-wrapper align-center\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/wing-chun.jpeg\" alt=\"pic_wing_chun\" data-slide-to=\"1\" data-bs-slide-to=\"1\">\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h3 class=\"mbr-title mbr-fonts-style mb-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title, .mbr-section-btn\" mbr-theme-style=\"display-2\">KUNG FUNG TRAINER</h3>\n                            <div class=\"mbr-section-btn\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showBtn\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-white-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Learn more<span class=\"effectHover\"></span></a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                \n                <div class=\"col-12 col-lg item features-image\" mbr-if=\"cardsAmount>2\">\n                    <div class=\"item-wrapper align-center\">\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/the-climate-reality-project-hb6uwq0i4mi-unsplash.jpg\" alt=\"Pic_SIMPLIFIED\" data-slide-to=\"1\" data-bs-slide-to=\"1\">\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h3 class=\"mbr-title mbr-fonts-style mb-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-title, .mbr-section-btn\" mbr-theme-style=\"display-2\">SIMPLIFIED ACADEMY</h3>\n                            <div class=\"mbr-section-btn\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showBtn\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-white-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Learn more<span class=\"effectHover\"></span></a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg item features-image\" mbr-if=\"cardsAmount>3\">\n                    <div class=\"item-